<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<properties resource="generatorConfig.properties" />
	<!--delin -->
	<!-- <classPathEntry location="D:\m2\1.7\pay\repository\com\oracle\jdbc\ojdbc\11.2\ojdbc-11.2.jar" 
		/> -->
	<!--wenzhe -->
	<classPathEntry
		location="C:\Users\yangc\repo\mysql\mysql-connector-java\5.1.30\mysql-connector-java-5.1.30.jar" />
	<!-- location="C:\Users\yangc\repo\com\oracle\jdbc\ojdbc\11.2\ojdbc-11.2.jar" 
		/> -->

	<context id="MysqlContext" targetRuntime="MyBatis3Simple"
		defaultModelType="flat">
		<property name="beginningDelimiter" value="`" />
		<property name="endingDelimiter" value="`" />

		<plugin type="${mapper.plugin}">
			<property name="mappers" value="${mapper.Mapper}" />
			<!-- caseSensitive默认false，当数据库表名区分大小写时，可以将该属性设置为true -->
			<property name="caseSensitive" value="true" />
		</plugin>
		<jdbcConnection driverClass="${jdbc.driverClassName}"
			connectionURL="${jdbc.url}" userId="${jdbc.username}" password="${jdbc.password}">
		</jdbcConnection>

		<javaModelGenerator targetPackage="${targetModelPackage}"
			targetProject="${targetJavaProject}" />

		<sqlMapGenerator targetPackage="${targetXMLPackage}"
			targetProject="${targetResourcesProject}" />

		<javaClientGenerator targetPackage="${targetMapperPackage}"
			targetProject="${targetJavaProject}" type="XMLMAPPER" />

		<!-- <table schema="acc" tableName="t_merchant_rate"> <generatedKey column="id" 
			sqlStatement="select TO_CHAR(SYSDATE, 'yymmddhh24mi')||SEQ_ACC_T_SHOP.nextval 
			from dual" /> </table> -->
		<table  tableName="city">
			<generatedKey column="id"
				sqlStatement="select TO_CHAR(SYSDATE, 'yymmddhh24mi')||SEQ_ACC_T_SHOP.nextval from dual" />
		</table>
	</context>
</generatorConfiguration>